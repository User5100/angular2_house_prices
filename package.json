{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "node ./dist/server/app.js",
    "test": "karma start",
    "clean:dist": "rimraf dist",
    "prebuild": "npm run clean:dist",
    "build": "webpack -p --env.prod --progress --profile --bail",
    "build:server": "grunt",
    "dev": "concurrently \"json-server --watch json_server/db.json\" \"webpack-dev-server --env.dev --inline --progress --port 8080\""
  },
